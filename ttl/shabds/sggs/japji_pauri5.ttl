    gg:gurmukhi "ਜਿਨਿ ਸੇਵਿਆ"@pa ;
    gg:transliteration "jin seviā"@en-Latn ;
    gg:translation "whoever has served Him"@en .

gg:sggs_japji_pauri5_tuk3_gu2 a gg:GlossUnit ;
    gg:index "2"^^xsd:integer ;
    gg:gurmukhi "ਤਿਨਿ ਪਾਇਆ ਮਾਨੁ"@pa ;
    gg:transliteration "tin pāiā mān"@en-Latn ;
