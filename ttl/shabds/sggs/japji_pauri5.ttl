    gg:index "1"^^xsd:integer ;
    gg:belongsTo gg:sggs_japji_pauri5 ;
    gg:gurmukhi "ਥਾਪਿਆ ਨ ਜਾਇ ਕੀਤਾ ਨ ਹੋਇ ॥"@pa ;
    gg:transliteration "Thāpiā na jāe, kītā na hoe."@en-Latn ;
    gg:translation "He cannot be installed; He cannot be made."@en ;
