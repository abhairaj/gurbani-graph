    gg:gurmukhi "ਥਾਪਿਆ ਨ ਜਾਇ"@pa ;
    gg:transliteration "thāpiā na jāe"@en-Latn ;
    gg:translation "He cannot be installed / set up"@en .

gg:sggs_japji_pauri5_tuk1_gu2 a gg:GlossUnit ;
    gg:index "2"^^xsd:integer ;
    gg:gurmukhi "ਕੀਤਾ ਨ ਹੋਇ"@pa ;
    gg:transliteration "kītā na hoe"@en-Latn ;
    gg:translation "He cannot be manufactured / made"@en .

