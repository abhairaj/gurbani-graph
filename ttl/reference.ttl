@prefix gg:  <http://gurbani-graph.org/core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix dct:   <http://purl.org/dc/terms/> .

#################################################################
### Scriptures
#################################################################

gg:SGGS a gg:Scripture ;
    rdfs:label "Sri Guru Granth Sahib Ji"@en .

gg:SDG a gg:Scripture ;
    rdfs:label "Sri Dasam Granth"@en .

#################################################################
### People
#################################################################

gg:GuruNanakDevJi a gg:Person ;
    rdfs:label "Guru Nanak Dev Ji"@en ;
    gg:role "Guru"@en .

gg:GuruArjanDevJi a gg:Person ;
    rdfs:label "Guru Arjan Dev Ji"@en ;
    gg:role "Guru"@en .

gg:GuruRamDasJi a gg:Person ;
    rdfs:label "Guru Ram Das Ji"@en ;
    gg:role "Guru"@en .

#################################################################
### Banis
#################################################################

gg:AnandSahib a gg:Bani ;
    gg:inScripture gg:SGGS ;
    rdfs:label "Anand Sahib"@en ;
    rdfs:comment "For bliss and liberation from the cycle of births/deaths"@en .

gg:AsaDiVar a gg:Bani ;
    # gg:inScripture gg:SGGS ;
    rdfs:label "Asa Di Var"@en ;
    rdfs:comment "A collection of hymns usually recited in the morning congregations at Gurdwaras."@en .

gg:BentiChaupai a gg:Bani ;
    gg:inScripture gg:SDS ;
    rdfs:label "Benti Chaupai (Chaupai Sahib)"@en ;
    rdfs:comment "A personal prayer for protection and strength"@en .

gg:DakhniOankar a gg:Bani ;
    gg:inScripture gg:SGGS ;
    rdfs:label "Dakhni Oankar"@en ;
    rdfs:comment "Longer philosophical composition"@en .

gg:JaapSahib a gg:Bani ;
    gg:inScripture gg:SDS ;
    rdfs:label "Jaap Sahib"@en ;
    rdfs:comment "Salutation to the Timeless Lord, protection"@en .

gg:JaitsreeVaar a gg:Bani ;
  gg:inScripture gg:SGGS ;
  rdfs:label "Jaitsree Ki Vaar (Mahalla 5)"@en ;
  rdfs:comment "Vaar in Raag Jaitsree by Guru Arjan Dev Ji, beginning on Ang 705 of Sri Guru Granth Sahib Ji."@en .

gg:JapJiSahib a gg:Bani ;
    gg:inScripture gg:SGGS ;
    rdfs:label "Jap Ji Sahib"@en ;
    rdfs:comment "Attaining knowledge of God (Brahm Gyan)"@en .

gg:KirtanSohila a gg:Bani ;
    gg:inScripture gg:SGGS ;
    rdfs:label "Kirtan Sohila"@en ;
    rdfs:comment "For peace before sleep and protection from negative thoughts/dreams"@en .

gg:Lavaan a gg:Bani ;
    gg:inScripture gg:SGGS ;
    rdfs:label "Lavaan"@en ;
    rdfs:comment "Wedding hymns recited during the Sikh marriage ceremony (Anand Karaj)"@en .

gg:RehrasSahib a gg:Bani ;
    gg:inScripture gg:SGGS ;
    rdfs:label "Rehras Sahib"@en ;
    rdfs:comment "Adds energy to one's being and brings prosperity"@en .

gg:ShabadHazaaray a gg:Bani ;
    gg:inScripture gg:SGGS ;
    rdfs:label "Shabad Hazaaray"@en ;
    rdfs:comment "An deep expression of devotion and desire for the Divine"@en .

gg:SiddhaGosh a gg:Bani ;
    gg:inScripture gg:SGGS ;
    rdfs:label "Siddha Gosh"@en ;
    rdfs:comment "Longer philosophical composition"@en .

gg:SukhmaniSahib a gg:Bani ;
    gg:inScripture gg:SGGS ;
    rdfs:label "Sukhmani Sahib"@en ;
    rdfs:comment "The \"Jewel of Peace\"; Composition of Guru Arjan Dev Ji in Raag Gauri, structured as 24 ashtpadis with preceding saloks."@en .

gg:TavPrasadSavaiye a gg:Bani ;
    gg:inScripture gg:SDS ;
    rdfs:label "Tav-Prasad Savaiye"@en ;
    rdfs:comment "Attaining worldly detachment, abolishing false practices"@en .


#################################################################
### Raags
#################################################################

gg:RaagAasaa a gg:Raag ;
    rdfs:label "ਆਸਾ"@pa .

gg:RaagJaitsree a gg:Raag ;
  rdfs:label "ਜੈਤਸਰੀ"@pa .

gg:RaagVadhans a gg:Raag ;
    rdfs:label "Vadhans"@en .

gg:RaagGauri a gg:Raag ;
  rdfs:label "ਗਉੜੀ"@pa .
