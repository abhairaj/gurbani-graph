#################################################################

gg:japji_pauri2_tuk5 a gg:Tuk ;
    gg:id "sggs_japji-pauri2_tuk5" ;
    gg:index "5"^^xsd:integer ;
    gg:belongsTo gg:japji_pauri2 ;
    gg:gurmukhi "ਹੁਕਮੈ ਅੰਦਰਿ ਸਭੁ ਕੋ ਬਾਹਰਿ ਹੁਕਮ ਨ ਕੋਇ ॥"@pa ;
    gg:transliteration "Hukmai andar sabh ko bāhar hukam na koe."@en-Latn ;
    gg:translation "All are within Hukam; none at all is outside His Command."@en ;
