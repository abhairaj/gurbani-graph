#################################################################
### Tuk 1
### ਹੁਕਮੀ ਹੋਵਨਿ ਆਕਾਰ ਹੁਕਮੁ ਨ ਕਹਿਆ ਜਾਈ ॥
#################################################################

gg:japji_pauri2_tuk1 a gg:Tuk ;
    gg:id "sggs_japji-pauri2_tuk1" ;
    gg:index "1"^^xsd:integer ;
    gg:belongsTo gg:japji_pauri2 ;
    gg:gurmukhi "ਹੁਕਮੀ ਹੋਵਨਿ ਆਕਾਰ ਹੁਕਮੁ ਨ ਕਹਿਆ ਜਾਈ ॥"@pa ;
    gg:transliteration "Hukmī hovan ākār hukam na kahiā jāī."@en-Latn ;
    gg:translation "By His Hukam, forms come into being; His Hukam cannot be fully described."@en ;
    gg:hasTheme gg:Hukam,
                   gg:CreationDestruction ;
    rdfs:comment "All manifest forms arise under Divine Command, yet the nature of Hukam itself transcends complete description or conceptual capture."@en ;
    gg:hasGlossUnit gg:japji_pauri2_tuk1_gu1,
                    gg:japji_pauri2_tuk1_gu2,
                    gg:japji_pauri2_tuk1_gu3 .

