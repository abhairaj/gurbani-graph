    gg:gurmukhi "ਹੋਵਨਿ ਆਕਾਰ"@pa ;
    gg:transliteration "hovan ākār"@en-Latn ;
    gg:translation "forms come into being; forms arise"@en .

### Tuk 1 – GlossUnit 3: ਹੁਕਮੁ ਨ ਕਹਿਆ ਜਾਈ

gg:japji_pauri2_tuk1_gu3 a gg:GlossUnit ;
    gg:index "3"^^xsd:integer ;
    gg:gurmukhi "ਹੁਕਮੁ ਨ ਕਹਿਆ ਜਾਈ"@pa ;
    gg:transliteration "hukam na kahiā jāī"@en-Latn ;
    gg:translation "Hukam cannot be fully spoken or described"@en .

