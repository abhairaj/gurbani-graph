.

gg:ang10_sopurkh_tuk6_gu1 a gg:GlossUnit ;
    gg:index "1"^^xsd:integer ;
    gg:gurmukhi "ਤੂੰ ਘਟ ਘਟ ਅੰਤਰਿ"@pa ;
    gg:transliteration "tūⁿ ghat ghat antar"@en-Latn ;
