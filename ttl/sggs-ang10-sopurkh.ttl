    dct:title "So Purkh"@en ;
    dct:title "ਸੋ ਪੁਰਖੁ"@pa ;
    gg:ang "10"^^xsd:integer ;
    gg:index "21"^^xsd:integer ;
    gg:hasAuthor gg:GuruRamDasJi ;
