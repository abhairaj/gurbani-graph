    gg:index "1"^^xsd:integer ;
    gg:gurmukhi "ਹਰਿ ਧਿਆਵਹੁ"@pa ;
    gg:transliteration "har dhi▫aavahu"@en-Latn ;
    gg:translation "meditate on the Lord"@en .

gg:ang10_sopurkh_tuk4_gu2 a gg:GlossUnit ;
    gg:index "2"^^xsd:integer ;
    gg:gurmukhi "ਸੰਤਹੁ ਜੀ"@pa ;
    gg:transliteration "sanṫahu jee"@en-Latn ;
    gg:translation "O saints, dear ones"@en .

gg:ang10_sopurkh_tuk4_gu3 a gg:GlossUnit ;
    gg:index "3"^^xsd:integer ;
    gg:gurmukhi "ਸਭਿ ਦੂਖ ਵਿਸਾਰਣਹਾਰਾ"@pa ;
    gg:transliteration "sabh ḋookʰ visaaraṇhaaraa"@en-Latn ;
    gg:translation "the dispeller of every sorrow"@en .

#################################################################
