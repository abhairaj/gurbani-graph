#################################################################
### Tuk 1
### ਸੋ ਪੁਰਖੁ ਨਿਰੰਜਨੁ ਹਰਿ ਪੁਰਖੁ ਨਿਰੰਜਨੁ ਹਰਿ ਅਗਮਾ ਅਗਮ ਅਪਾਰਾ ॥
#################################################################

gg:ang10_sopurkh_tuk1 a gg:Tuk ;
    gg:id "sggs_ang10-sopurkh_tuk1" ;
    gg:index "1"^^xsd:integer ;
    gg:belongsTo gg:ang10_sopurkh ;
    gg:gurmukhi "ਸੋ ਪੁਰਖੁ ਨਿਰੰਜਨੁ ਹਰਿ ਪੁਰਖੁ ਨਿਰੰਜਨੁ ਹਰਿ ਅਗਮਾ ਅਗਮ ਅਪਾਰਾ ॥"@pa ;
    gg:transliteration "So purakʰ niranjan har purakʰ niranjan har agmaa agam apaaraa."@en-Latn ;
    gg:translation "That Primal Being is immaculate and pure; the Lord, the Primal Being, is stainless, beyond reach and beyond all limits."@en ;
    gg:hasGlossUnit
        gg:ang10_sopurkh_tuk1_gu1,
        gg:ang10_sopurkh_tuk1_gu2,
        gg:ang10_sopurkh_tuk1_gu3
.

gg:ang10_sopurkh_tuk1_gu1 a gg:GlossUnit ;
    gg:index "1"^^xsd:integer ;
    gg:gurmukhi "ਸੋ ਪੁਰਖੁ ਨਿਰੰਜਨੁ"@pa ;
    gg:transliteration "So purakh niranjan"@en-Latn ;
    gg:translation "that primal, immaculate Being"@en .

gg:ang10_sopurkh_tuk1_gu2 a gg:GlossUnit ;
    gg:index "2"^^xsd:integer ;
    gg:gurmukhi "ਹਰਿ ਪੁਰਖੁ ਨਿਰੰਜਨੁ"@pa ;
    gg:transliteration "har purakh niranjan"@en-Latn ;
    gg:translation "the Lord, the immaculate primal Being"@en .

gg:ang10_sopurkh_tuk1_gu3 a gg:GlossUnit ;
    gg:index "3"^^xsd:integer ;
    gg:gurmukhi "ਹਰਿ ਅਗਮਾ ਅਗਮ ਅਪਾਰਾ"@pa ;
    gg:transliteration "har agmaa agam apaaraa"@en-Latn ;
    gg:translation "the Lord who is inaccessible, unreachable and boundless"@en .

#################################################################
### Tuk 2
### ਸਭਿ ਧਿਆਵਹਿ ਸਭਿ ਧਿਆਵਹਿ ਤੁਧੁ ਜੀ ਹਰਿ ਸਚੇ ਸਿਰਜਣਹਾਰਾ ॥
#################################################################

gg:ang10_sopurkh_tuk2 a gg:Tuk ;
    gg:id "sggs_ang10-sopurkh_tuk2" ;
    gg:index "2"^^xsd:integer ;
    gg:belongsTo gg:ang10_sopurkh ;
    gg:gurmukhi "ਸਭਿ ਧਿਆਵਹਿ ਸਭਿ ਧਿਆਵਹਿ ਤੁਧੁ ਜੀ ਹਰਿ ਸਚੇ ਸਿਰਜਣਹਾਰਾ ॥"@pa ;
    gg:transliteration "Sabʰ ḋʰi▫aavahi sabʰ ḋʰi▫aavahi ṫuḋʰ jee har saché sirjaṇhaaraa."@en-Latn ;
    gg:translation "All meditate, all truly meditate on You, O Dear Lord, the True Creator."@en ;
    gg:hasGlossUnit
        gg:ang10_sopurkh_tuk2_gu1,
        gg:ang10_sopurkh_tuk2_gu2,
        gg:ang10_sopurkh_tuk2_gu3
.

gg:ang10_sopurkh_tuk2_gu1 a gg:GlossUnit ;
    gg:index "1"^^xsd:integer ;
    gg:gurmukhi "ਸਭਿ ਧਿਆਵਹਿ ਸਭਿ ਧਿਆਵਹਿ"@pa ;
    gg:transliteration "sabh dhi▫aavahi sabh dhi▫aavahi"@en-Latn ;
    gg:translation "all meditate, all meditate"@en .

gg:ang10_sopurkh_tuk2_gu2 a gg:GlossUnit ;
    gg:index "2"^^xsd:integer ;
    gg:gurmukhi "ਤੁਧੁ ਜੀ"@pa ;
    gg:transliteration "tudh jee"@en-Latn ;
    gg:translation "on You, O Dear Lord"@en .

gg:ang10_sopurkh_tuk2_gu3 a gg:GlossUnit ;
    gg:index "3"^^xsd:integer ;
    gg:gurmukhi "ਹਰਿ ਸਚੇ ਸਿਰਜਣਹਾਰਾ"@pa ;
    gg:transliteration "har sache sirjaṇhaaraa"@en-Latn ;
    gg:translation "the True Lord, Creator of all"@en .

#################################################################
### Tuk 3
### ਸਭਿ ਜੀਅ ਤੁਮਾਰੇ ਜੀ ਤੂੰ ਜੀਆ ਕਾ ਦਾਤਾਰਾ ॥
#################################################################

gg:ang10_sopurkh_tuk3 a gg:Tuk ;
    gg:id "sggs_ang10-sopurkh_tuk3" ;
    gg:index "3"^^xsd:integer ;
    gg:belongsTo gg:ang10_sopurkh ;
    gg:gurmukhi "ਸਭਿ ਜੀਅ ਤੁਮਾਰੇ ਜੀ ਤੂੰ ਜੀਆ ਕਾ ਦਾਤਾਰਾ ॥"@pa ;
    gg:transliteration "Sabʰ jee▫a ṫumaaré jee ṫooⁿ jee▫aa kaa ḋaaṫaaraa."@en-Latn ;
    gg:translation "All living beings are Yours; You, O Lord, are the giver of life to every soul."@en ;
    gg:hasGlossUnit
        gg:ang10_sopurkh_tuk3_gu1,
        gg:ang10_sopurkh_tuk3_gu2,
        gg:ang10_sopurkh_tuk3_gu3
.

gg:ang10_sopurkh_tuk3_gu1 a gg:GlossUnit ;
    gg:index "1"^^xsd:integer ;
    gg:gurmukhi "ਸਭਿ ਜੀਅ ਤੁਮਾਰੇ ਜੀ"@pa ;
    gg:transliteration "sabh jee▫a tumaaré jee"@en-Latn ;
    gg:translation "all beings belong to You, dear Lord"@en .

gg:ang10_sopurkh_tuk3_gu2 a gg:GlossUnit ;
    gg:index "2"^^xsd:integer ;
    gg:gurmukhi "ਤੂੰ ਜੀਆ ਕਾ"@pa ;
    gg:transliteration "tooⁿ jee▫aa kaa"@en-Latn ;
    gg:translation "You are of all lives"@en .

gg:ang10_sopurkh_tuk3_gu3 a gg:GlossUnit ;
    gg:index "3"^^xsd:integer ;
    gg:gurmukhi "ਦਾਤਾਰਾ"@pa ;
    gg:transliteration "ḋaaṫaaraa"@en-Latn ;
    gg:translation "the giver, the sustainer"@en .

#################################################################
### Tuk 4
### ਹਰਿ ਧਿਆਵਹੁ ਸੰਤਹੁ ਜੀ ਸਭਿ ਦੂਖ ਵਿਸਾਰਣਹਾਰਾ ॥
#################################################################

gg:ang10_sopurkh_tuk4 a gg:Tuk ;
    gg:id "sggs_ang10-sopurkh_tuk4" ;
    gg:index "4"^^xsd:integer ;
    gg:belongsTo gg:ang10_sopurkh ;
    gg:gurmukhi "ਹਰਿ ਧਿਆਵਹੁ ਸੰਤਹੁ ਜੀ ਸਭਿ ਦੂਖ ਵਿਸਾਰਣਹਾਰਾ ॥"@pa ;
    gg:transliteration "Har ḋʰi▫aavahu sanṫahu jee sabʰ ḋookʰ visaaraṇhaaraa."@en-Latn ;
    gg:translation "Meditate on the Lord, O saints; He is the One who removes all sorrow."@en ;
