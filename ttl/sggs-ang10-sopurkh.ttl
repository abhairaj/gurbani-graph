#################################################################

gg:ang10_sopurkh_tuk6 a gg:Tuk ;
    gg:id "sggs_ang10-sopurkh_tuk6" ;
    gg:index "6"^^xsd:integer ;
    gg:belongsTo gg:ang10_sopurkh ;
