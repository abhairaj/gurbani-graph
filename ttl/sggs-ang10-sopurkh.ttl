gg:ang10_sopurkh_tuk5_gu1 a gg:GlossUnit ;
    gg:index "1"^^xsd:integer ;
    gg:gurmukhi "ਹਰਿ ਆਪੇ ਠਾਕੁਰੁ"@pa ;
    gg:transliteration "har aapé tʰaakur"@en-Latn ;
    gg:translation "the Lord Himself is the Master"@en .

gg:ang10_sopurkh_tuk5_gu2 a gg:GlossUnit ;
    gg:index "2"^^xsd:integer ;
    gg:gurmukhi "ਹਰਿ ਆਪੇ ਸੇਵਕੁ ਜੀ"@pa ;
    gg:transliteration "har aapé sévak jee"@en-Latn ;
    gg:translation "the Lord Himself is also the servant"@en .

gg:ang10_sopurkh_tuk5_gu3 a gg:GlossUnit ;
    gg:index "3"^^xsd:integer ;
    gg:gurmukhi "ਕਿਆ ਨਾਨਕ ਜੰਤ ਵਿਚਾਰਾ"@pa ;
    gg:transliteration "ki▫aa Naanak janṫ vichaaraa"@en-Latn ;
    gg:translation "how insignificant are creatures like us, says Nanak"@en .

