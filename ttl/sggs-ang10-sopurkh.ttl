gg:ang10_sopurkh_tuk6_gu2 a gg:GlossUnit ;
    gg:index "2"^^xsd:integer ;
    gg:gurmukhi "ਸਰਬ ਨਿਰੰਤਰਿ ਜੀ"@pa ;
    gg:transliteration "sarab nirantar jee"@en-Latn ;
    gg:translation "pervading all, continuously"@en .

gg:ang10_sopurkh_tuk6_gu3 a gg:GlossUnit ;
