:gSGGS a :Granth.

:a555 a :Ang;
    :partOf :gSGSS;
    :index 555;
    :gurmukhi "ਪਪਪ"^^xsd:string;
    :translation "555"^^xsd:string;
    :hasLine :a555_l14, :a555_l15, :a555_l16, :a555_l17, :a555_l18, :a555_l19, :a555_l20, :a555_l21.

:a555_l14 a :Line;
    :index 14;
    :gurmukhi "ਜਾ ਆਪਿ ਕ੍ਰਿਪਾਲੁ ਹੋਵੈ ਹਰਿ ਸੁਆਮੀ ਤਾ ਆਪਣਾਂ ਨਾਉ ਹਰਿ ਆਪਿ ਜਪਾਵੈ ॥"^^xsd:string;
    :transliteration "Jā āp kirpāl hovai har suāmī, tā āpṇāṅ nāu har āp japāvai."^^xsd:string;
    :translation "When the Lord, the Master, becomes merciful, then He Himself causes His Name to be chanted."^^xsd:string;
    :hasPhrase :a555_l14_p01, :a555_l14_p02, :a555_l14_p03, :a555_l14_p04, :a555_l14_p05, :a555_l14_p06.

:a555_l14_p01 a :Phrase;
    :index 1;
    :gurmukhi "ਜਾ"^^xsd:string;
    :transliteration "Jā"^^xsd:string;
    :translation "When"^^xsd:string.

:a555_l14_p02 a :Phrase;
    :index 2;
    :gurmukhi "ਆਪਿ ਕ੍ਰਿਪਾਲੁ ਹੋਵੈ"^^xsd:string;
    :transliteration "āp kirpāl hovai"^^xsd:string;
    :translation "the Lord Himself becomes gracious/merciful"^^xsd:string.

:a555_l14_p03 a :Phrase;
    :index 3;
    :gurmukhi "ਹਰਿ ਸੁਆਮੀ"^^xsd:string;
    :transliteration "har suāmī"^^xsd:string;
    :translation "the Divine Master, the Lord"^^xsd:string.

:a555_l14_p04 a :Phrase;
    :index 4;
    :gurmukhi "ਤਾ"^^xsd:string;
    :transliteration "tā"^^xsd:string;
    :translation "then"^^xsd:string.

:a555_l14_p05 a :Phrase;
    :index 5;
    :gurmukhi "ਆਪਣਾਂ ਨਾਉ"^^xsd:string;
    :transliteration "āpaṇāṅ nāu"^^xsd:string;
    :translation "His own Name"^^xsd:string.

:a555_l14_p06 a :Phrase;
    :index 6;
    :gurmukhi "ਹਰਿ ਆਪਿ ਜਪਾਵੈ"^^xsd:string;
    :transliteration "har āp japāvai"^^xsd:string;
    :translation "He Himself causes to be uttered/meditated upon"^^xsd:string.

:a555_l15 a :Line;
    :index 15;
    :gurmukhi "ਆਪੇ ਸਤਿਗੁਰੁ ਮੇਲਿ ਸੁਖੁ ਦੇਵੈ ਆਪਣਾਂ ਸੇਵਕੁ ਆਪਿ ਹਰਿ ਭਾਵੈ ॥"^^xsd:string;
    :transliteration "Āpe satigur mel sukh devai, āpaṇāṅ sevak āp har bhāvai."^^xsd:string;
    :translation "The Lord Himself unites one with the True Guru, blesses with peace, and He Himself becomes pleased with His servant."^^xsd:string;
    :hasPhrase :a555_l15_p01, :a555_l15_p02, :a555_l15_p03, :a555_l15_p04, :a555_l15_p05.

:a555_l15_p01 a :Phrase;
    :index 1;
    :gurmukhi "ਆਪੇ"^^xsd:string;
    :transliteration "āpe"^^xsd:string;
    :translation "Himself (the Lord alone)"^^xsd:string.

:a555_l15_p02 a :Phrase;
    :index 2;
    :gurmukhi "ਸਤਿਗੁਰੁ ਮੇਲਿ"^^xsd:string;
    :transliteration "satigur mel"^^xsd:string;
    :translation "causes union with the True Guru"^^xsd:string.

:a555_l15_p03 a :Phrase;
    :index 3;
    :gurmukhi "ਸੁਖੁ ਦੇਵੈ"^^xsd:string;
    :transliteration "sukh devai"^^xsd:string;
    :translation "grants peace/bliss"^^xsd:string.

:a555_l15_p04 a :Phrase;
    :index 4;
    :gurmukhi "ਆਪਣਾਂ ਸੇਵਕੁ"^^xsd:string;
    :transliteration "āpaṇāṅ sevak"^^xsd:string;
    :translation "His own devotee/servant"^^xsd:string.

:a555_l15_p05 a :Phrase;
    :index 5;
    :gurmukhi "ਆਪਿ ਹਰਿ ਭਾਵੈ"^^xsd:string;
    :transliteration "āp har bhāvai"^^xsd:string;
    :translation "the Lord Himself is pleased with"^^xsd:string.
