    shabd:phraseIndex "1"^^xsd:integer ;
    shabd:phraseGurmukhi "ਹੁਕਮੀ"@pa ;
    shabd:phraseTransliteration "Hukmī"@en-Latn ;
    shabd:phraseGlossEn "by His Command; under Hukam"@en .

