        sggs:ang10_sopurkh_line15,
        sggs:ang10_sopurkh_line16,
        sggs:ang10_sopurkh_line17,
        sggs:ang10_sopurkh_line18,
        sggs:ang10_sopurkh_line19,
        sggs:ang10_sopurkh_line20,
        sggs:ang10_sopurkh_line21,
        sggs:ang10_sopurkh_line22,
