#################################################################
### Line 1
### ਸੋ ਪੁਰਖੁ ਨਿਰੰਜਨੁ ਹਰਿ ਪੁਰਖੁ ਨਿਰੰਜਨੁ ਹਰਿ ਅਗਮਾ ਅਗਮ ਅਪਾਰਾ ॥
#################################################################

sggs:ang10_sopurkh_line1 a shabd:Line ;
    shabd:id "sggs-ang10-sopurkh-line1" ;
    shabd:index "1"^^xsd:integer ;
    shabd:belongsTo sggs:ang10_sopurkh ;
    shabd:gurmukhi "ਸੋ ਪੁਰਖੁ ਨਿਰੰਜਨੁ ਹਰਿ ਪੁਰਖੁ ਨਿਰੰਜਨੁ ਹਰਿ ਅਗਮਾ ਅਗਮ ਅਪਾਰਾ ॥"@pa ;
    shabd:transliteration "So purakʰ niranjan har purakʰ niranjan har agmaa agam apaaraa."@en-Latn ;
    shabd:translation "That Primal Being is immaculate and pure; the Lord, the Primal Being, is stainless, beyond reach and beyond all limits."@en ;
    shabd:hasGlossUnit
        sggs:ang10_sopurkh_line1_gu1,
        sggs:ang10_sopurkh_line1_gu2,
        sggs:ang10_sopurkh_line1_gu3
.

sggs:ang10_sopurkh_line1_gu1 a shabd:GlossUnit ;
    shabd:index "1"^^xsd:integer ;
    shabd:gurmukhi "ਸੋ ਪੁਰਖੁ ਨਿਰੰਜਨੁ"@pa ;
    shabd:transliteration "So purakh niranjan"@en-Latn ;
    shabd:translation "that primal, immaculate Being"@en .

sggs:ang10_sopurkh_line1_gu2 a shabd:GlossUnit ;
    shabd:index "2"^^xsd:integer ;
    shabd:gurmukhi "ਹਰਿ ਪੁਰਖੁ ਨਿਰੰਜਨੁ"@pa ;
    shabd:transliteration "har purakh niranjan"@en-Latn ;
    shabd:translation "the Lord, the immaculate primal Being"@en .

sggs:ang10_sopurkh_line1_gu3 a shabd:GlossUnit ;
    shabd:index "3"^^xsd:integer ;
    shabd:gurmukhi "ਹਰਿ ਅਗਮਾ ਅਗਮ ਅਪਾਰਾ"@pa ;
    shabd:transliteration "har agmaa agam apaaraa"@en-Latn ;
    shabd:translation "the Lord who is inaccessible, unreachable and boundless"@en .

#################################################################
### Line 2
### ਸਭਿ ਧਿਆਵਹਿ ਸਭਿ ਧਿਆਵਹਿ ਤੁਧੁ ਜੀ ਹਰਿ ਸਚੇ ਸਿਰਜਣਹਾਰਾ ॥
#################################################################

sggs:ang10_sopurkh_line2 a shabd:Line ;
    shabd:id "sggs-ang10-sopurkh-line2" ;
    shabd:index "2"^^xsd:integer ;
    shabd:belongsTo sggs:ang10_sopurkh ;
    shabd:gurmukhi "ਸਭਿ ਧਿਆਵਹਿ ਸਭਿ ਧਿਆਵਹਿ ਤੁਧੁ ਜੀ ਹਰਿ ਸਚੇ ਸਿਰਜਣਹਾਰਾ ॥"@pa ;
    shabd:transliteration "Sabʰ ḋʰi▫aavahi sabʰ ḋʰi▫aavahi ṫuḋʰ jee har saché sirjaṇhaaraa."@en-Latn ;
    shabd:translation "All meditate, all truly meditate on You, O Dear Lord, the True Creator."@en ;
    shabd:hasGlossUnit
        sggs:ang10_sopurkh_line2_gu1,
        sggs:ang10_sopurkh_line2_gu2,
        sggs:ang10_sopurkh_line2_gu3
.

sggs:ang10_sopurkh_line2_gu1 a shabd:GlossUnit ;
    shabd:index "1"^^xsd:integer ;
    shabd:gurmukhi "ਸਭਿ ਧਿਆਵਹਿ ਸਭਿ ਧਿਆਵਹਿ"@pa ;
    shabd:transliteration "sabh dhi▫aavahi sabh dhi▫aavahi"@en-Latn ;
    shabd:translation "all meditate, all meditate"@en .

sggs:ang10_sopurkh_line2_gu2 a shabd:GlossUnit ;
    shabd:index "2"^^xsd:integer ;
    shabd:gurmukhi "ਤੁਧੁ ਜੀ"@pa ;
    shabd:transliteration "tudh jee"@en-Latn ;
    shabd:translation "on You, O Dear Lord"@en .

sggs:ang10_sopurkh_line2_gu3 a shabd:GlossUnit ;
    shabd:index "3"^^xsd:integer ;
    shabd:gurmukhi "ਹਰਿ ਸਚੇ ਸਿਰਜਣਹਾਰਾ"@pa ;
    shabd:transliteration "har sache sirjaṇhaaraa"@en-Latn ;
    shabd:translation "the True Lord, Creator of all"@en .

#################################################################
### Line 3
### ਸਭਿ ਜੀਅ ਤੁਮਾਰੇ ਜੀ ਤੂੰ ਜੀਆ ਕਾ ਦਾਤਾਰਾ ॥
#################################################################

sggs:ang10_sopurkh_line3 a shabd:Line ;
    shabd:id "sggs-ang10-sopurkh-line3" ;
    shabd:index "3"^^xsd:integer ;
    shabd:belongsTo sggs:ang10_sopurkh ;
    shabd:gurmukhi "ਸਭਿ ਜੀਅ ਤੁਮਾਰੇ ਜੀ ਤੂੰ ਜੀਆ ਕਾ ਦਾਤਾਰਾ ॥"@pa ;
    shabd:transliteration "Sabʰ jee▫a ṫumaaré jee ṫooⁿ jee▫aa kaa ḋaaṫaaraa."@en-Latn ;
    shabd:translation "All living beings are Yours; You, O Lord, are the giver of life to every soul."@en ;
    shabd:hasGlossUnit
        sggs:ang10_sopurkh_line3_gu1,
        sggs:ang10_sopurkh_line3_gu2,
        sggs:ang10_sopurkh_line3_gu3
.

sggs:ang10_sopurkh_line3_gu1 a shabd:GlossUnit ;
    shabd:index "1"^^xsd:integer ;
    shabd:gurmukhi "ਸਭਿ ਜੀਅ ਤੁਮਾਰੇ ਜੀ"@pa ;
    shabd:transliteration "sabh jee▫a tumaaré jee"@en-Latn ;
    shabd:translation "all beings belong to You, dear Lord"@en .

sggs:ang10_sopurkh_line3_gu2 a shabd:GlossUnit ;
    shabd:index "2"^^xsd:integer ;
    shabd:gurmukhi "ਤੂੰ ਜੀਆ ਕਾ"@pa ;
    shabd:transliteration "tooⁿ jee▫aa kaa"@en-Latn ;
    shabd:translation "You are of all lives"@en .

sggs:ang10_sopurkh_line3_gu3 a shabd:GlossUnit ;
    shabd:index "3"^^xsd:integer ;
    shabd:gurmukhi "ਦਾਤਾਰਾ"@pa ;
    shabd:transliteration "ḋaaṫaaraa"@en-Latn ;
    shabd:translation "the giver, the sustainer"@en .

#################################################################
### Line 4
### ਹਰਿ ਧਿਆਵਹੁ ਸੰਤਹੁ ਜੀ ਸਭਿ ਦੂਖ ਵਿਸਾਰਣਹਾਰਾ ॥
#################################################################

sggs:ang10_sopurkh_line4 a shabd:Line ;
    shabd:id "sggs-ang10-sopurkh-line4" ;
    shabd:index "4"^^xsd:integer ;
    shabd:belongsTo sggs:ang10_sopurkh ;
    shabd:gurmukhi "ਹਰਿ ਧਿਆਵਹੁ ਸੰਤਹੁ ਜੀ ਸਭਿ ਦੂਖ ਵਿਸਾਰਣਹਾਰਾ ॥"@pa ;
    shabd:transliteration "Har ḋʰi▫aavahu sanṫahu jee sabʰ ḋookʰ visaaraṇhaaraa."@en-Latn ;
    shabd:translation "Meditate on the Lord, O saints; He is the One who removes all sorrow."@en ;
    shabd:hasGlossUnit
        sggs:ang10_sopurkh_line4_gu1,
        sggs:ang10_sopurkh_line4_gu2,
        sggs:ang10_sopurkh_line4_gu3
.

sggs:ang10_sopurkh_line4_gu1 a shabd:GlossUnit ;
    shabd:index "1"^^xsd:integer ;
    shabd:gurmukhi "ਹਰਿ ਧਿਆਵਹੁ"@pa ;
    shabd:transliteration "har dhi▫aavahu"@en-Latn ;
    shabd:translation "meditate on the Lord"@en .

sggs:ang10_sopurkh_line4_gu2 a shabd:GlossUnit ;
    shabd:index "2"^^xsd:integer ;
    shabd:gurmukhi "ਸੰਤਹੁ ਜੀ"@pa ;
    shabd:transliteration "sanṫahu jee"@en-Latn ;
    shabd:translation "O saints, dear ones"@en .

sggs:ang10_sopurkh_line4_gu3 a shabd:GlossUnit ;
    shabd:index "3"^^xsd:integer ;
    shabd:gurmukhi "ਸਭਿ ਦੂਖ ਵਿਸਾਰਣਹਾਰਾ"@pa ;
    shabd:transliteration "sabh ḋookʰ visaaraṇhaaraa"@en-Latn ;
    shabd:translation "the dispeller of every sorrow"@en .

#################################################################
### Line 5
### ਹਰਿ ਆਪੇ ਠਾਕੁਰੁ ਹਰਿ ਆਪੇ ਸੇਵਕੁ ਜੀ ਕਿਆ ਨਾਨਕ ਜੰਤ ਵਿਚਾਰਾ ॥੧॥
#################################################################

sggs:ang10_sopurkh_line5 a shabd:Line ;
    shabd:id "sggs-ang10-sopurkh-line5" ;
    shabd:index "5"^^xsd:integer ;
    shabd:belongsTo sggs:ang10_sopurkh ;
    shabd:gurmukhi "ਹਰਿ ਆਪੇ ਠਾਕੁਰੁ ਹਰਿ ਆਪੇ ਸੇਵਕੁ ਜੀ ਕਿਆ ਨਾਨਕ ਜੰਤ ਵਿਚਾਰਾ ॥੧॥"@pa ;
    shabd:transliteration "Har aapé tʰaakur har aapé sévak jee ki▫aa Naanak janṫ vichaaraa. ||1||"@en-Latn ;
    shabd:translation "The Lord Himself is the Master, the Lord Himself is the servant; what can these poor creatures do or understand, says Nanak? ||1||"@en ;
    shabd:hasGlossUnit
        sggs:ang10_sopurkh_line5_gu1,
        sggs:ang10_sopurkh_line5_gu2,
        sggs:ang10_sopurkh_line5_gu3
.

sggs:ang10_sopurkh_line5_gu1 a shabd:GlossUnit ;
    shabd:index "1"^^xsd:integer ;
    shabd:gurmukhi "ਹਰਿ ਆਪੇ ਠਾਕੁਰੁ"@pa ;
    shabd:transliteration "har aapé tʰaakur"@en-Latn ;
    shabd:translation "the Lord Himself is the Master"@en .

sggs:ang10_sopurkh_line5_gu2 a shabd:GlossUnit ;
    shabd:index "2"^^xsd:integer ;
    shabd:gurmukhi "ਹਰਿ ਆਪੇ ਸੇਵਕੁ ਜੀ"@pa ;
    shabd:transliteration "har aapé sévak jee"@en-Latn ;
    shabd:translation "the Lord Himself is also the servant"@en .

sggs:ang10_sopurkh_line5_gu3 a shabd:GlossUnit ;
    shabd:index "3"^^xsd:integer ;
    shabd:gurmukhi "ਕਿਆ ਨਾਨਕ ਜੰਤ ਵਿਚਾਰਾ"@pa ;
    shabd:transliteration "ki▫aa Naanak janṫ vichaaraa"@en-Latn ;
    shabd:translation "how insignificant are creatures like us, says Nanak"@en .

#################################################################
### Line 6
### ਤੂੰ ਘਟ ਘਟ ਅੰਤਰਿ ਸਰਬ ਨਿਰੰਤਰਿ ਜੀ ਹਰਿ ਏਕੋ ਪੁਰਖੁ ਸਮਾਣਾ ॥
#################################################################

sggs:ang10_sopurkh_line6 a shabd:Line ;
    shabd:id "sggs-ang10-sopurkh-line6" ;
    shabd:index "6"^^xsd:integer ;
    shabd:belongsTo sggs:ang10_sopurkh ;
    shabd:gurmukhi "ਤੂੰ ਘਟ ਘਟ ਅੰਤਰਿ ਸਰਬ ਨਿਰੰਤਰਿ ਜੀ ਹਰਿ ਏਕੋ ਪੁਰਖੁ ਸਮਾਣਾ ॥"@pa ;
    shabd:transliteration "Ṫooⁿ gʰat gʰat anṫar sarab niranṫar jee har éko purakʰ samaaṇaa."@en-Latn ;
    shabd:translation "You dwell within each and every heart; You pervade all, O Lord, the One Being present everywhere."@en ;
    shabd:hasGlossUnit
        sggs:ang10_sopurkh_line6_gu1,
        sggs:ang10_sopurkh_line6_gu2,
        sggs:ang10_sopurkh_line6_gu3
.

sggs:ang10_sopurkh_line6_gu1 a shabd:GlossUnit ;
    shabd:index "1"^^xsd:integer ;
    shabd:gurmukhi "ਤੂੰ ਘਟ ਘਟ ਅੰਤਰਿ"@pa ;
    shabd:transliteration "tūⁿ ghat ghat antar"@en-Latn ;
    shabd:translation "You are within each and every heart"@en .

sggs:ang10_sopurkh_line6_gu2 a shabd:GlossUnit ;
    shabd:index "2"^^xsd:integer ;
    shabd:gurmukhi "ਸਰਬ ਨਿਰੰਤਰਿ ਜੀ"@pa ;
    shabd:transliteration "sarab nirantar jee"@en-Latn ;
    shabd:translation "pervading all, continuously"@en .

sggs:ang10_sopurkh_line6_gu3 a shabd:GlossUnit ;
    shabd:index "3"^^xsd:integer ;
    shabd:gurmukhi "ਹਰਿ ਏਕੋ ਪੁਰਖੁ ਸਮਾਣਾ"@pa ;
    shabd:transliteration "har eko purakh samaaṇaa"@en-Latn ;
    shabd:translation "as the one Lord, the Primal Being, present everywhere"@en .

#################################################################
### Line 7
### ਇਕਿ ਦਾਤੇ ਇਕਿ ਭੇਖਾਰੀ ਜੀ ਸਭਿ ਤੇਰੇ ਚੋਜ ਵਿਡਾਣਾ ॥
#################################################################

sggs:ang10_sopurkh_line7 a shabd:Line ;
    shabd:id "sggs-ang10-sopurkh-line7" ;
    shabd:index "7"^^xsd:integer ;
    shabd:belongsTo sggs:ang10_sopurkh ;
    shabd:gurmukhi "ਇਕਿ ਦਾਤੇ ਇਕਿ ਭੇਖਾਰੀ ਜੀ ਸਭਿ ਤੇਰੇ ਚੋਜ ਵਿਡਾਣਾ ॥"@pa ;
    shabd:transliteration "Ik ḋaaṫé ik bʰékʰaaree jee sabʰ ṫéré choj vidaaṇaa."@en-Latn ;
    shabd:translation "Some appear as givers and some as beggars, but all of this is only Your wondrous play."@en ;
    shabd:hasGlossUnit
        sggs:ang10_sopurkh_line7_gu1,
        sggs:ang10_sopurkh_line7_gu2,
        sggs:ang10_sopurkh_line7_gu3
.

sggs:ang10_sopurkh_line7_gu1 a shabd:GlossUnit ;
    shabd:index "1"^^xsd:integer ;
    shabd:gurmukhi "ਇਕਿ ਦਾਤੇ ਇਕਿ ਭੇਖਾਰੀ ਜੀ"@pa ;
    shabd:transliteration "ik ḋaaṫe ik bʰékʰaaree jee"@en-Latn ;
    shabd:translation "some are seen as givers, some as beggars"@en .

sggs:ang10_sopurkh_line7_gu2 a shabd:GlossUnit ;
    shabd:index "2"^^xsd:integer ;
    shabd:gurmukhi "ਸਭਿ ਤੇਰੇ"@pa ;
    shabd:transliteration "sabh tére"@en-Latn ;
    shabd:translation "all are Yours"@en .

sggs:ang10_sopurkh_line7_gu3 a shabd:GlossUnit ;
    shabd:index "3"^^xsd:integer ;
    shabd:gurmukhi "ਚੋਜ ਵਿਡਾਣਾ"@pa ;
    shabd:transliteration "choj vidaaṇaa"@en-Latn ;
    shabd:translation "wondrous play, amazing drama"@en .

#################################################################
### Line 8
### ਤੂੰ ਆਪੇ ਦਾਤਾ ਆਪੇ ਭੁਗਤਾ ਜੀ ਹਉ ਤੁਧੁ ਬਿਨੁ ਅਵਰੁ ਨ ਜਾਣਾ ॥
#################################################################

sggs:ang10_sopurkh_line8 a shabd:Line ;
    shabd:id "sggs-ang10-sopurkh-line8" ;
    shabd:index "8"^^xsd:integer ;
    shabd:belongsTo sggs:ang10_sopurkh ;
    shabd:gurmukhi "ਤੂੰ ਆਪੇ ਦਾਤਾ ਆਪੇ ਭੁਗਤਾ ਜੀ ਹਉ ਤੁਧੁ ਬਿਨੁ ਅਵਰੁ ਨ ਜਾਣਾ ॥"@pa ;
    shabd:transliteration "Ṫooⁿ aapé ḋaaṫaa aapé bʰugṫaa jee ha▫o ṫuḋʰ bin avar na jaaṇaa."@en-Latn ;
    shabd:translation "You Yourself are the Giver, and You Yourself are the Enjoyer; apart from You, I know no other."@en ;
    shabd:hasGlossUnit
        sggs:ang10_sopurkh_line8_gu1,
        sggs:ang10_sopurkh_line8_gu2,
        sggs:ang10_sopurkh_line8_gu3
.

sggs:ang10_sopurkh_line8_gu1 a shabd:GlossUnit ;
    shabd:index "1"^^xsd:integer ;
    shabd:gurmukhi "ਤੂੰ ਆਪੇ ਦਾਤਾ ਆਪੇ ਭੁਗਤਾ ਜੀ"@pa ;
    shabd:transliteration "tūⁿ aapé ḋaaṫaa aapé bʰugṫaa jee"@en-Latn ;
    shabd:translation "You Yourself are both the giver and the enjoyer"@en .

sggs:ang10_sopurkh_line8_gu2 a shabd:GlossUnit ;
    shabd:index "2"^^xsd:integer ;
    shabd:gurmukhi "ਹਉ ਤੁਧੁ ਬਿਨੁ"@pa ;
    shabd:transliteration "hao tuḋʰ bin"@en-Latn ;
