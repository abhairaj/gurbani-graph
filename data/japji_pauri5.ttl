    shabd:lineId "sggs:japji-pauri5-line6" ;
    shabd:index "6"^^xsd:integer ;
    shabd:belongsTo sggs:japji_pauri5 ;
    shabd:gurmukhi "ਦੁਖੁ ਪਰਹਰਿ ਸੁਖੁ ਘਰਿ ਲੈ ਜਾਇ ॥"@pa ;
    shabd:transliteration "Dukh parhar, sukh ghar lai jāi."@en-Latn ;
    shabd:translationEn "Thus suffering is driven out, and peace is brought home."@en ;
    shabd:hasTheme shabd:SufferingRemoved ;
    shabd:commentaryKeypoint "Such singing, listening, and loving contemplation uproot suffering and bring abiding peace within."@en ;
    shabd:hasPhrase sggs:japji_pauri5_line6_p1,
