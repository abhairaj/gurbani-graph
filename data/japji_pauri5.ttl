    shabd:phraseTransliteration "gurmukh nādaṅ"@en-Latn ;
    shabd:phraseGlossEn "for the Gurmukh, there is the Divine sound (Nāḍ)"@en .

sggs:japji_pauri5_line7_p2 a shabd:Phrase ;
    shabd:phraseIndex "2"^^xsd:integer ;
    shabd:phraseGurmukhi "ਗੁਰਮੁਖਿ ਵੇਦੰ"@pa ;
    shabd:phraseTransliteration "gurmukh vedaṅ"@en-Latn ;
    shabd:phraseGlossEn "for the Gurmukh, true scriptural wisdom"@en .

sggs:japji_pauri5_line7_p3 a shabd:Phrase ;
    shabd:phraseIndex "3"^^xsd:integer ;
    shabd:phraseGurmukhi "ਗੁਰਮੁਖਿ ਰਹਿਆ ਸਮਾਈ"@pa ;
    shabd:phraseTransliteration "gurmukh rahiā samāī"@en-Latn ;
    shabd:phraseGlossEn "for the Gurmukh, He is realized as all-pervading"@en .

