    shabd:phraseIndex "1"^^xsd:integer ;
    shabd:phraseGurmukhi "ਦੁਖੁ ਪਰਹਰਿ"@pa ;
    shabd:phraseTransliteration "dukh parhar"@en-Latn ;
    shabd:phraseGlossEn "driving away suffering"@en .

sggs:japji_pauri5_line6_p2 a shabd:Phrase ;
    shabd:phraseIndex "2"^^xsd:integer ;
    shabd:phraseGurmukhi "ਸੁਖੁ ਘਰਿ"@pa ;
    shabd:phraseTransliteration "sukh ghar"@en-Latn ;
    shabd:phraseGlossEn "the peace / happiness of home (within)"@en .

