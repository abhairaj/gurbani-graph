    shabd:phraseIndex "3"^^xsd:integer ;
    shabd:phraseGurmukhi "ਲੈ ਜਾਇ"@pa ;
    shabd:phraseTransliteration "lai jāi"@en-Latn ;
    shabd:phraseGlossEn "is taken and established there"@en .

