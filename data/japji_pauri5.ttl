    shabd:phraseGurmukhi "ਕਰਤੋਟਿ"@pa ;
    shabd:phraseTransliteration "karṭoṭ"@en-Latn ;
    shabd:phraseGlossEn "beyond reckoning; beyond calculation"@en .

