#################################################################
### Japji Sahib – Pauri 5
#################################################################

sggs:japji_pauri5 a shabd:Pauri ;
    shabd:shabdId "sggs-japji-pauri5" ;
    dct:title "Japji Sahib – Pauri 5"@en ;
    shabd:titleEn "Japji Sahib – Pauri 5"@en ;
    shabd:sequenceInAng "5"^^xsd:integer ;
    shabd:hasAuthor sggs:GuruNanakDevJi ;
    shabd:inScripture sggs:SGGS ;
    shabd:hasAng sggs:Ang1 ;
    shabd:hasTheme shabd:UncreatedDivine,
                   shabd:NiranjanTheme,
                   shabd:ServiceAndHonor,
                   shabd:PraiseSinging,
                   shabd:ListeningAndLove,
                   shabd:SufferingRemoved,
                   shabd:GurmukhPath,
                   shabd:GuruAsDivineForm,
                   shabd:IneffabilityTheme,
                   shabd:OneGiverTheme ;
    shabd:overallCommentaryEn "Pauri 5 proclaims that the Lord is not installed or manufactured; He is self-existent and immaculate. Serving such a One brings true honor. The pauri urges singing, listening, and lovingly holding the Divine within, which removes suffering and brings peace home. Through the Guru-centered way, Divine sound, knowledge, and pervasion are realized; the Guru is honored as the embodiment of Divine powers. Ultimately, the Guru gives one essential understanding: there is one Giver of all beings, who must never be forgotten."@en ;
    shabd:hasLine sggs:japji_pauri5_line1,
                  sggs:japji_pauri5_line2,
                  sggs:japji_pauri5_line3,
                  sggs:japji_pauri5_line4,
                  sggs:japji_pauri5_line5,
                  sggs:japji_pauri5_line6,
                  sggs:japji_pauri5_line7,
                  sggs:japji_pauri5_line8,
                  sggs:japji_pauri5_line9,
                  sggs:japji_pauri5_line10,
                  sggs:japji_pauri5_line11 .

#################################################################
### Line 1
### ਥਾਪਿਆ ਨ ਜਾਇ ਕੀਤਾ ਨ ਹੋਇ ॥
#################################################################

sggs:japji_pauri5_line1 a shabd:Line ;
    shabd:lineId "sggs-japji-pauri5-line1" ;
    shabd:index "1"^^xsd:integer ;
    shabd:belongsTo sggs:japji_pauri5 ;
    shabd:gurmukhi "ਥਾਪਿਆ ਨ ਜਾਇ ਕੀਤਾ ਨ ਹੋਇ ॥"@pa ;
    shabd:transliteration "Thāpiā na jāe, kītā na hoe."@en-Latn ;
    shabd:translationEn "He cannot be installed; He cannot be made."@en ;
    shabd:hasTheme shabd:UncreatedDivine ;
    shabd:commentaryKeypoint "The Divine is not a constructed idol or artifact; He is beyond being made by human hands."@en ;
    shabd:hasPhrase sggs:japji_pauri5_line1_p1,
                    sggs:japji_pauri5_line1_p2 .

sggs:japji_pauri5_line1_p1 a shabd:Phrase ;
    shabd:phraseIndex "1"^^xsd:integer ;
    shabd:phraseGurmukhi "ਥਾਪਿਆ ਨ ਜਾਇ"@pa ;
    shabd:phraseTransliteration "thāpiā na jāe"@en-Latn ;
    shabd:phraseGlossEn "He cannot be installed / set up"@en .

sggs:japji_pauri5_line1_p2 a shabd:Phrase ;
    shabd:phraseIndex "2"^^xsd:integer ;
    shabd:phraseGurmukhi "ਕੀਤਾ ਨ ਹੋਇ"@pa ;
    shabd:phraseTransliteration "kītā na hoe"@en-Latn ;
    shabd:phraseGlossEn "He cannot be manufactured / made"@en .

#################################################################
### Line 2
### ਆਪੇ ਆਪਿ ਨਿਰੰਜਨੁ ਸੋਇ ॥
#################################################################

sggs:japji_pauri5_line2 a shabd:Line ;
    shabd:lineId "sggs:japji-pauri5-line2" ;
    shabd:index "2"^^xsd:integer ;
    shabd:belongsTo sggs:japji_pauri5 ;
    shabd:gurmukhi "ਆਪੇ ਆਪਿ ਨਿਰੰਜਨੁ ਸੋਇ ॥"@pa ;
    shabd:transliteration "Āpe āp niranjan soe."@en-Latn ;
    shabd:translationEn "He Himself is by Himself, immaculate and pure."@en ;
    shabd:hasTheme shabd:UncreatedDivine,
                   shabd:NiranjanTheme ;
    shabd:commentaryKeypoint "The Lord is self-existent and free from the stain of Maya."@en ;
    shabd:hasPhrase sggs:japji_pauri5_line2_p1,
                    sggs:japji_pauri5_line2_p2 .

sggs:japji_pauri5_line2_p1 a shabd:Phrase ;
    shabd:phraseIndex "1"^^xsd:integer ;
    shabd:phraseGurmukhi "ਆਪੇ ਆਪਿ"@pa ;
    shabd:phraseTransliteration "āpe āp"@en-Latn ;
    shabd:phraseGlossEn "He Himself, by Himself"@en .

sggs:japji_pauri5_line2_p2 a shabd:Phrase ;
    shabd:phraseIndex "2"^^xsd:integer ;
    shabd:phraseGurmukhi "ਨਿਰੰਜਨੁ ਸੋਇ"@pa ;
    shabd:phraseTransliteration "niranjan soe"@en-Latn ;
    shabd:phraseGlossEn "that One is immaculate, beyond taint"@en .

#################################################################
### Line 3
### ਜਿਨਿ ਸੇਵਿਆ ਤਿਨਿ ਪਾਇਆ ਮਾਨੁ ॥
#################################################################

sggs:japji_pauri5_line3 a shabd:Line ;
    shabd:lineId "sggs:japji-pauri5-line3" ;
    shabd:index "3"^^xsd:integer ;
    shabd:belongsTo sggs:japji_pauri5 ;
    shabd:gurmukhi "ਜਿਨਿ ਸੇਵਿਆ ਤਿਨਿ ਪਾਇਆ ਮਾਨੁ ॥"@pa ;
    shabd:transliteration "Jin seviā tin pāiā mān."@en-Latn ;
    shabd:translationEn "Whoever serves Him, that one receives honor."@en ;
    shabd:hasTheme shabd:ServiceAndHonor ;
    shabd:commentaryKeypoint "True honor comes not from status but from serving the immaculate One."@en ;
    shabd:hasPhrase sggs:japji_pauri5_line3_p1,
                    sggs:japji_pauri5_line3_p2 .

sggs:japji_pauri5_line3_p1 a shabd:Phrase ;
    shabd:phraseIndex "1"^^xsd:integer ;
    shabd:phraseGurmukhi "ਜਿਨਿ ਸੇਵਿਆ"@pa ;
    shabd:phraseTransliteration "jin seviā"@en-Latn ;
    shabd:phraseGlossEn "whoever has served Him"@en .

sggs:japji_pauri5_line3_p2 a shabd:Phrase ;
    shabd:phraseIndex "2"^^xsd:integer ;
    shabd:phraseGurmukhi "ਤਿਨਿ ਪਾਇਆ ਮਾਨੁ"@pa ;
    shabd:phraseTransliteration "tin pāiā mān"@en-Latn ;
    shabd:phraseGlossEn "that one obtains honor"@en .

#################################################################
### Line 4
### ਨਾਨਕ ਗਾਵੀਐ ਗੁਣੀ ਨਿਧਾਨੁ ॥
#################################################################

sggs:japji_pauri5_line4 a shabd:Line ;
    shabd:lineId "sggs:japji-pauri5-line4" ;
    shabd:index "4"^^xsd:integer ;
    shabd:belongsTo sggs:japji_pauri5 ;
    shabd:gurmukhi "ਨਾਨਕ ਗਾਵੀਐ ਗੁਣੀ ਨਿਧਾਨੁ ॥"@pa ;
    shabd:transliteration "Nānak gāvīai guṇī niḏhān."@en-Latn ;
    shabd:translationEn "O Nanak, sing of Him, the treasure of virtues."@en ;
    shabd:hasTheme shabd:PraiseSinging ;
    shabd:commentaryKeypoint "Guru Nanak urges continual singing of the Virtue-Treasure, the Lord filled with countless excellences."@en ;
    shabd:hasPhrase sggs:japji_pauri5_line4_p1,
                    sggs:japji_pauri5_line4_p2 .

sggs:japji_pauri5_line4_p1 a shabd:Phrase ;
    shabd:phraseIndex "1"^^xsd:integer ;
    shabd:phraseGurmukhi "ਨਾਨਕ ਗਾਵੀਐ"@pa ;
    shabd:phraseTransliteration "Nānak gāvīai"@en-Latn ;
    shabd:phraseGlossEn "O Nanak, let us sing"@en .

sggs:japji_pauri5_line4_p2 a shabd:Phrase ;
    shabd:phraseIndex "2"^^xsd:integer ;
    shabd:phraseGurmukhi "ਗੁਣੀ ਨਿਧਾਨੁ"@pa ;
    shabd:phraseTransliteration "guṇī niḏhān"@en-Latn ;
    shabd:phraseGlossEn "the treasure of virtues"@en .

#################################################################
### Line 5
### ਗਾਵੀਐ ਸੁਣੀਐ ਮਨਿ ਰਖੀਐ ਭਾਉ ॥
#################################################################

sggs:japji_pauri5_line5 a shabd:Line ;
    shabd:lineId "sggs:japji-pauri5-line5" ;
