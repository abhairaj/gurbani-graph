    shabd:phraseGurmukhi "ਗਾਵੀਐ ਸੁਣੀਐ"@pa ;
    shabd:phraseTransliteration "gāvīai suṇīai"@en-Latn ;
    shabd:phraseGlossEn "let us sing and let us listen"@en .

sggs:japji_pauri5_line5_p2 a shabd:Phrase ;
    shabd:phraseIndex "2"^^xsd:integer ;
    shabd:phraseGurmukhi "ਮਨਿ ਰਖੀਐ"@pa ;
    shabd:phraseTransliteration "man rakhīai"@en-Latn ;
    shabd:phraseGlossEn "keep (it) in the mind"@en .

sggs:japji_pauri5_line5_p3 a shabd:Phrase ;
    shabd:phraseIndex "3"^^xsd:integer ;
    shabd:phraseGurmukhi "ਭਾਉ"@pa ;
    shabd:phraseTransliteration "bẖāo"@en-Latn ;
    shabd:phraseGlossEn "loving devotion; affectionate awe"@en .

#################################################################
