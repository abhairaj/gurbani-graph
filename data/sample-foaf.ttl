@prefix ex: <http://example.org/ontology#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Define a person
ex:Alice a foaf:Person ;
    foaf:name "Alice Smith" ;
    foaf:age "30"^^xsd:integer ;
    foaf:knows ex:Bob .

# Define another person
ex:Bob a foaf:Person ;
    foaf:name "Bob Johnson" ;
    foaf:age "25"^^xsd:integer ;
    foaf:knows ex:Alice, ex:Charlie .

# Define a third person
ex:Charlie a foaf:Person ;
    foaf:name "Charlie Brown" ;
    foaf:age "35"^^xsd:integer .

# Define a movie
ex:Movie123 a ex:Movie ;
    ex:title "The Graph Adventure" ;
    ex:director ex:Alice .

# Define a genre
ex:Fantasy a ex:Genre ;
    ex:name "Fantasy" .

# Relate the movie to the genre
ex:Movie123 ex:hasGenre ex:Fantasy .